#include <stdio.h>
#include <stdlib.h>
int main() {
	int n,i=0,j=1,t;
	int* a;
	printf("Введите размер последовательности\n");
	scanf("%d", &n);
	if (n <= 0)
		printf("Данные введены некорректно");
	else {
	    printf("Введите последовательность\n");
		a = (int*)malloc(n * sizeof(int));
		for (i=0; i < n; i++) 
			scanf("%d", &a[i]);
		for (i = 1; i < n; i++) {
			for (j; j != 0; j -= 1) 
				if (a[j] < a[j - 1]) {
					t = a[j - 1];
					a[j - 1] = a[j];
					a[j] = t;
				}
				j=i+1;
		}
		for (i = 0; i < n; i++)
			printf("%d ", a[i]);
		free(a);
	}
	return 0;
}
